<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="258.0" prefWidth="441.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bookclubdesktopcli.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="ButtonTilt" alignment="CENTER" layoutX="100.0" mnemonicParsing="false" onAction="#onUpdate" prefHeight="26.0" prefWidth="162.0" text="Tiltás/Tiltás visszavonása" />
   <TableView fx:id="Table" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="idOszlop" prefWidth="75.0" text="id" />
       <TableColumn fx:id="nameoszlop" prefWidth="75.0" text="name" />
         <TableColumn fx:id="genderOszlop" prefWidth="75.0" text="gender" />
         <TableColumn fx:id="birth_dateOszlop" prefWidth="75.0" text="birth_date" />
         <TableColumn fx:id="bannedOszlop" prefWidth="75.0" text="banned" />
     </columns>
   </TableView>

    <Label fx:id="welcomeText" />
</VBox>
